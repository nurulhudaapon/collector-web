const zx = @import("zx");

pub fn Layout(ctx: zx.LayoutContext, children: zx.Component) zx.Component {
    return (
        <html @allocator={ctx.arena} lang="en-US">
            <head>
                <title>collector</title>
                <link rel="stylesheet" href="/styles.css" />
            </head>
            <body>
                {(children)}
            </body>
        </html>
    );
}
